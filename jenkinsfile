pipeline {
    agent {
      node {
	label "linux && java11"
      }
    }
    stages {
        stage("Hello aldi") {
            steps {
                echo("Hello Alditya")
            }
        }
    }

    post {
      always {
	echo "I will always say hello again"
      }
      success {
	echo "Yey, success"
      }
      failure {
	echo "Oh no, failure"
      }
      cleanup {
	echo "Don't care success or error"
      }
     }

}
